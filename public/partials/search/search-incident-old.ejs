 <div class="container-fluid" ng-controller="searchCtrl">
                <div class="side-body">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="alert alert-danger col-md-12" id="danger-alert" style="display: none">
                                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                <center> <strong> Oops! </strong> No Incident found. Please try again! </center>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xs-12">
                            <div class="card">
                                <div class="card-header">
                                    <div class="card-title">
                                        <span class="title">Search for Incidents</span>
                                        <span class="description"></span>
                                    </div>
                                </div>
                                <div class="card-body no-padding">
                                    <div class="step card-no-padding">
                                        <ul class="nav nav-tabs nav-justified" role="tablist">
                                            <li role="step" class="active">
                                                <a href="#step1-2" id="step1-tab" role="tab" data-toggle="tab" aria-controls="home" aria-expanded="true">
                                                    <div class="icon fa fa-search"></div>
                                                    <div class="step-title">
                                                        <div class="title">Search by Incident ID</div>
                                                        <div class="description">Enter Incident ID</div>
                                                    </div>
                                                </a>
                                            </li>
                                            <li role="step" >
                                                <a href="#step2-2" role="tab" id="step2-tab" data-toggle="tab" aria-controls="profile">
                                                    <div class="icon fa fa-calendar"></div>
                                                    <div class="step-title">
                                                        <div class="title">Search by date</div>
                                                        <div class="description">Enter date range</div>
                                                    </div>
                                                </a>
                                            </li>
                                            <li role="step">
                                                <a href="#step3-2" role="tab" id="step3-tab" data-toggle="tab" aria-controls="profile">
                                                    <div class="icon fa fa-mobile"></div>
                                                    <div class="step-title">
                                                        <div class="title">Search by Application</div>
                                                        <div class="description">Choose Application</div>
                                                    </div>
                                                </a>
                                            </li>
                                        </ul>
                                        <div class="tab-content">
                                            <div role="tabpanel" class="tab-pane fade in active" id="step1-2" aria-labelledby="home-tab">

                                                <div class="row">
                                                    <div class="col-sm-12">

                                                            <form class="form-horizontal" ng-submit="searchbyId(id);">
                                                                <div class="form-group">
                                                                      <label for="incidentID" class="col-sm-4 control-label">Incident ID</label>
                                                                      <div class="col-sm-4">
                                                                          <input type="text" required ng-model="id"  class="form-control" id="incidentID" value="IM005000000" />
                                                                          <button type="submit" class="btn btn-md btn-block btn-info">Search by Incident ID</button>

                                                                      </div>

                                                                </div>
                                                            </form>
                                                    </div>
                                                </div>
                                                <div ng-show="incidentResult">
                                                    <h2> Results </h2>
                                                    <hr />
                                                    <!-- New Row -->
                                                     <div class="row">
                                                        <div class="col-sm-12">

                                                            <table datatable="" class="row-border hover">
                                                                <thead>
                                                                <tr>
                                                                    <th>ID</th>
                                                                    <th>Title</th>
                                                                    <th>Application</th>
                                                                    <th>Type</th>
                                                                    <th>Status</th>
                                                                    <th>Priority</th>
                                                                    <th>Reported Date</th>
                                                                </tr>
                                                                </thead>
                                                                <tbody>
                                                                <tr>
                                                                    <td><b><u><a href="/incident-detail/{{incidentData.incidentID}}/">{{incidentData.incidentID}}</a></u></b></td>
                                                                    <td>{{incidentData.title}}</td>
                                                                    <td>{{incidentData.application}}</td>
                                                                    <td>{{incidentData.incidentType}}</td>
                                                                    <td>{{incidentData.status}}</td>
                                                                    <td>{{incidentData.priority}}</td>
                                                                    <td>{{incidentData.reportedDate}}</td>
                                                                </tr>

                                                                </tbody>
                                                            </table>
                                                        </div>
                                                     </div>
                                                </div>

                                            </div>
                                            <div role="tabpanel" class="tab-pane fade" id="step2-2" aria-labelledby="profile-tab">

                                                    <div class="row">
                                                        <div class="col-sm-12">

                                                            <form class="form-horizontal" ng-submit="searchByDate(dateObj);">

                                                                <div class="form-group">
                                                                    <label for="fromDate" class="col-sm-2 control-label">From date</label>
                                                                    <div class="col-sm-2">
                                                                        <input type="date"  required ng-model="dateObj.fromDate" class="form-control" id="fromDate"  />
                                                                    </div>
                                                                    <label for="toDate" class="col-sm-2 control-label">To Date</label>
                                                                    <div class="col-sm-2">
                                                                        <button type="submit" class="btn btn-md btn-block btn-success">Search by Date</button>
                                                                    </div>

                                                                </div>
                                                            </form>

                                                        </div>
                                                    </div>
                                                     <div >
                                                    <h2> Results </h2>
                                                    <hr />
                                                    <!-- New Row -->
                                                    <div class="row">
                                                        <div class="col-sm-12">

                                                            <table datatable="ng" id="dateRangeTable" class="row-border hover">
                                                                <thead>
                                                                <tr>
                                                                    <th>ID</th>
                                                                    <th>Title</th>
                                                                    <th>Application</th>
                                                                    <th>Type</th>
                                                                    <th>Status</th>
                                                                    <th>Priority</th>
                                                                    <th>Reported Date</th>
                                                                </tr>
                                                                </thead>
                                                                <tbody>
                                                                <tr ng-repeat="search in searchDate">
                                                                    <td><b><u><a href="/incident-detail/{{search.incidentID}}/">{{search.incidentID}}</a></u></b></td>
                                                                    <td>{{search.title}}</td>
                                                                    <td>{{search.application}}</td>
                                                                    <td>{{search.incidentType}}</td>
                                                                    <td>{{search.status}}</td>
                                                                    <td>{{search.priority}}</td>
                                                                    <td>{{search.reportedDate}}</td>
                                                                </tr>

                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                 </div>

                                            </div>

                                            <div role="tabpanel" class="tab-pane fade" id="step3-2" aria-labelledby="dropdown1-tab">

                                                <div class="row">
                                                    <div class="col-sm-12">

                                                        <form class="form-horizontal" ng-submit="searchByAppln(data);">
                                                            <div class="form-group">
                                                                <label for="applicationSelect" class="col-sm-4 control-label">Application</label>
                                                                <div class="col-sm-4">
                                                                    <select ng-model="data.application" class="form-control" id="applicationSelect">

                                                                        <option value="Instore Server">Server</option>
                                                                        <option value="Instore PDA">PDA</option>
                                                                        <option value="Server and PDA">Server and PDA </option>
                                                                    </select>
                                                                    <button type="submit" class="btn btn-md btn-block btn-warning">Search by Application</button>
                                                                </div>

                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                                <div >
                                                    <h2> Results </h2>
                                                    <hr />
                                                    <!-- New Row -->

                                                     <div class="row">
                                                        <div class="col-sm-12">
                                                            <table datatable="ng" id="applicationTable" class="row-border hover">
                                                                <thead>
                                                                <tr>
                                                                    <th>ID</th>
                                                                    <th>Title</th>
                                                                    <th>Application</th>
                                                                    <th>Type</th>
                                                                    <th>Status</th>
                                                                    <th>Priority</th>
                                                                    <th>Reported Date</th>
                                                                </tr>
                                                                </thead>
                                                                <tbody>
                                                                <tr ng-repeat="app in searchApp">
                                                                    <td><b><u><a href="/incident-detail/{{app.incidentID}}/">{{app.incidentID}}</a></u></b></td>
                                                                    <td>{{app.title}}</td>
                                                                    <td>{{app.application}}</td>
                                                                    <td>{{app.incidentType}}</td>
                                                                    <td>{{app.status}}</td>
                                                                    <td>{{app.priority}}</td>
                                                                    <td>{{app.reportedDate}}</td>
                                                                </tr>

                                                                </tbody>
                                                            </table>


                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>